<?xml version="1.0" encoding="utf-8" ?>
<Patch>
<!-- Stuffed SewageOutlet -->
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Dubs Bad Hygiene</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Normal</success>
			<operations>
			<li Class="PatchOperationRemove">
				<xpath>Defs/ThingDef[defName = "SewageOutlet"]/costList</xpath>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>Defs/ThingDef[defName = "SewageOutlet"]</xpath>
				<value>
					<costStuffCount>50</costStuffCount>
					<stuffCategories>
						<li>Stony</li>
						<li>Metallic</li>
					</stuffCategories>
				</value>
			</li>
			</operations>
		</match> 
	</Operation>
</Patch>

